{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "nodenext",
        "strict": true,
        "resolveJsonModule": true,
        "allowJs": true,
        "strictPropertyInitialization": false,
        "noImplicitOverride": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    }
}
